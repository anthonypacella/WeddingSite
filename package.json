  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "dependencies": {
    "16": "^0.0.2",
    "bulma-pagination-react": "^0.1.1",
    "node": "^16.13.1",
    "react-select": "^5.3.2"
  },
  "engines": { 
    "node": ">=16.13.1"
  }
}
